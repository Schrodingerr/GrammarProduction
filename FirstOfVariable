#include<stdio.h>
#include<conio.h>
#include<string.h>
#include<ctype.h>
int main()
{
    char gr[10];
    char first[10];
    int k=3,j=0;
    char *ret;
    printf("Enter grammar");
    scanf("%s",gr);
    while(gr[k]!='\0')
    {
        //ret=memchr(gr,'>',strlen(gr));
        //if(gr[k]>='a' || gr[k]<='z')
        if(islower(gr[k]))
        {
            first[j]=gr[k];
            ++j;
        }
        ++k;
        
    }
    printf(first);

    return 0;
}

